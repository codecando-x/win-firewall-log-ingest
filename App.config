<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=token" >
            <section name="FirewallLogIngestService.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=token" requirePermission="false" />
        </sectionGroup>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=token" >
            <section name="FirewallLogIngestService.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=token" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="FirewallLogIngestService.Properties.Settings.pfirewallConnectionString"
            connectionString="Data Source=DATABASE;Initial Catalog=pfirewall;Integrated Security=True;TrustServerCertificate=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="token" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <applicationSettings>
    <FirewallLogIngestService.Properties.Settings>
      <setting name="MaxMindCityPath" serializeAs="String">
        <value>C:\Users\computer\repos\data\GeoLite2-City.mmdb</value>
      </setting>
      <setting name="MaxMindCountryPath" serializeAs="String">
        <value>C:\Users\computer\repos\data\GeoLite2-Country.mmdb</value>
      </setting>
      <setting name="MaxMindASNPath" serializeAs="String">
        <value>C:\Users\computer\repos\data\GeoLite2-ASN.mmdb</value>
      </setting>
    </FirewallLogIngestService.Properties.Settings>
  </applicationSettings>
  <userSettings>
    <FirewallLogIngestService.Properties.Settings>
      <setting name="lastRecIndex" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="FirewallLogFilePath" serializeAs="String">
        <value>C:\Windows\System32\LogFiles\Firewall\pfirewall.log</value>
      </setting>
      <setting name="FirewallLogFileSize" serializeAs="String">
        <value>0</value>
      </setting>
    </FirewallLogIngestService.Properties.Settings>
  </userSettings>
</configuration>